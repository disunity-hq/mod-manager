{
  "name": "disunity_manager",
  "version": "0.0.1",
  "main": "dist/electron.bundle.js",
  "license": "MIT",
  "scripts": {
    "webpack_main_dev": "webpack --config webpack.config.main.dev.js",
    "webpack_renderer_dev": "webpack --config webpack.config.renderer.dev.js",
    "webpack_main_prod": "webpack --config webpack.config.main.prod.js",
    "webpack_renderer_prod": "webpack --config webpack.config.renderer.prod.js",
    "dev": "npm run webpack_main_dev && webpack-dev-server --config webpack.config.renderer.dev.js",
    "storybook": "start-storybook",
    "build": "npm run webpack_main_prod && npm run webpack_renderer_prod && electron-builder"
  },
  "devDependencies": {
    "@storybook/addon-info": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@types/storybook-addon-jsx": "^5.4.3",
    "@types/storybook__react": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^2.1.1",
    "electron-builder": "^20.39.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.17.0",
    "react-docgen-typescript-loader": "^3.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.4.3",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "build": {
    "appId": "com.disunity.manager",
    "files": [
      "dist/bundle.js",
      "dist/electron.bundle.js",
      "dist/index.html"
    ],
    "directories": {
      "output": "build"
    }
  },
  "dependencies": {
    "@types/node": "^11.13.9",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "@types/react-router-dom": "^4.3.2",
    "electron": "4.1.4",
    "antd": "^3.16.6"
  }
}
